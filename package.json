{
  "name": "slacksync",
  "version": "1.1.0",
  "description": "Sync SFU Maillists with a Slack team",
  "main": "./dist/slacksync",
  "scripts": {
    "build": "rm -rf dist && babel src --ignore src/lib/__test__ --out-dir dist",
    "test": "ava -v",
    "lint": "eslint src",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "bin": {
    "slacksync": "src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfu/slacksync.git"
  },
  "author": "Graham Ballantyne <grahamb@sfu.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfu/slacksync/issues"
  },
  "ava": {
    "files": [
      "src/**/__test__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "prettier": {
    "singleQuote": true
  },
  "homepage": "https://github.com/sfu/slacksync#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "eslint": "4.19.1",
    "eslint-config-unobtrusive": "1.2.2",
    "prettier": "1.11.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "dashdash": "1.14.1",
    "dedent-js": "1.0.1",
    "moment": "2.22.0",
    "pluralize": "7.0.0",
    "slack": "6.1.0"
  }
}
