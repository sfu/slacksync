{
  "name": "slacksync",
  "version": "1.1.0",
  "description": "Sync SFU Maillists with a Slack team",
  "main": "./dist/slacksync",
  "scripts": {
    "build": "rm -rf dist && babel src --ignore src/lib/__test__ --out-dir dist",
    "test": "ava -v",
    "lint": "eslint src",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "bin": {
    "slacksync": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfu/slacksync.git"
  },
  "author": "Graham Ballantyne <grahamb@sfu.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfu/slacksync/issues"
  },
  "ava": {
    "files": [
      "src/**/__test__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "prettier": {
    "singleQuote": true
  },
  "homepage": "https://github.com/sfu/slacksync#readme",
  "devDependencies": {
    "ava": "0.24.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-dedent": "2.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "4.12.1",
    "eslint-config-unobtrusive": "1.2.0",
    "prettier": "1.9.1"
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel-runtime": "6.26.0",
    "dashdash": "1.14.1",
    "moment": "2.19.3",
    "pluralize": "7.0.0",
    "slack": "6.1.0"
  }
}
