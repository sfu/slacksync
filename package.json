{
  "name": "slacksync",
  "version": "1.0.5",
  "description": "Sync SFU Maillists with a Slack team",
  "main": "./dist/slacksync",
  "scripts": {
    "build": "rm -rf dist && babel src --ignore src/lib/__test__ --out-dir dist",
    "test": "ava -v",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm run build"
  },
  "bin": {
    "slacksync": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfu/slacksync.git"
  },
  "author": "Graham Ballantyne <grahamb@sfu.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfu/slacksync/issues"
  },
  "ava": {
    "files": [
      "src/**/__test__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "homepage": "https://github.com/sfu/slacksync#readme",
  "devDependencies": {
    "ava": "0.14.0",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.4",
    "babel-plugin-add-module-exports": "0.1.4",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-dedent": "2.0.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "eslint": "2.8.0"
  },
  "dependencies": {
    "axios": "0.11.0",
    "babel-runtime": "6.6.1",
    "dashdash": "1.13.1",
    "moment": "2.13.0",
    "pluralize": "1.2.1",
    "slack": "6.1.0"
  }
}
